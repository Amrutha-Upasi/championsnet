{% for photo in photos %}
    <div class="photo-wrapper">
      <a class="photo-thumb" target="_blank" title="" href="">
       <span class="photo-thumb-wrapper">
         <i data-photo-id="{{ photo.id }} " style="width: {{ settings.thumb_width }}px; height:{{ settings.thumb_height }}px;;"></i>
       </span>
      </a>
    </div>
{% endfor %}
